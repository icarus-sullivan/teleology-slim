{
  "name": "@teleology/slim",
  "version": "3.0.6",
  "description": "A tiny javascript template library",
  "homepage": "https://github.com/teleology-io/teleology-slim#readme",
  "bugs": {
    "url": "https://github.com/teleology-io/teleology-slim/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/teleology-io/teleology-slim.git"
  },
  "main": "lib/index.js",
  "bin": {
    "slim": "lib/cli.js"
  },
  "files": [
    "lib/*"
  ],
  "scripts": {
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "test": "jest",
    "test:cli": "node ./lib/cli.js -f sample/cli.slim -i input.json --partner parent company",
    "sample": "export PARTNER_COMPANY='Fictional Company' && node sample/index.js",
    "prepare": "tsc"
  },
  "keywords": [
    "template",
    "js",
    "slim",
    "tiny"
  ],
  "author": "Chris Sullivan",
  "license": "MIT",
  "devDependencies": {
    "eslint": "^7.6.0",
    "eslint-config-airbnb-base": "^14.2.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-config-teleology": "1.0.1",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-prettier": "^3.1.4",
    "jest": "^27.5.1",
    "prettier": "^2.0.5",
    "typescript": "^4.6.3"
  },
  "dependencies": {
    "buffer": "^6.0.3"
  }
}
