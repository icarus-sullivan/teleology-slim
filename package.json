{
  "name": "@teleology/slim",
  "version": "2.0.1",
  "description": "A tiny javascript template library",
  "repository": "https://github.com/icarus-sullivan/teleology-slim.git",
  "main": "lib/index.js",
  "bin": {
    "slim": "lib/cli.js"
  },
  "files": [
    "lib/*"
  ],
  "scripts": {
